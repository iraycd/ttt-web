{
    "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react",
      "module": "es6",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "outDir": "./build",
      "preserveConstEnums": true,
      "target": "es5"
    },
    "include": ["./src/**/*.ts", "./src/**/*.tsx"],
    "exclude": [
      "node_modules"
    ]
  }